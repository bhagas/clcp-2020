<body>
    <% include ../template/header.ejs %>

    <section class="section-one">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-2 col-lg-2 p-l-0 p-r-0 visible-md visible-lg">
              <div style="width: 100%;height: 100vh;background-color:#159B75;">
                <div class="row">
                  <div class="col-md-12 col-lg-12">
                    <div style="width: 100%;background-color:  #39C99C;padding:  15px;border-bottom: 6px solid rgba(0,0,0,0.2);">
                      <center><img src="/images/template/Logo Kab Pekalongan.png"></center>
                      <h5 class="m-b-0 text-center"><strong>PEMERINTAH KABUPATEN PEKALONGAN</strong></h5>
                      <h6 class="m-t-5 m-b-0 text-center"><strong>SISTEM INFORMASI RTRW KABUPATEN PEKALONGAN</strong></h6>
                    </div>
                  </div>
                </div>

