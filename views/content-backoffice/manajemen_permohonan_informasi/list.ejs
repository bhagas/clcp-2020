<% include ../../template-backoffice/header.ejs %>

<!-- BEGIN PAGE CONTENT -->
<div class="page-content">
  <div class="header">
    <h2><strong>Data Permohonan Informasi</strong></h2>
    <div class="breadcrumb-wrapper">
      <ol class="breadcrumb">
        <li><a href="/backoffice">Dashboard</a>
        </li>
        <li class="active">Data Permohonan Informasi</li>
      </ol>
    </div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <div class="panel">
        <div class="panel-header header-line">
           <!-- <a class="btn btn-primary btn-embossed" href="/manajemen_per/insert" role="button"><i class="fa fa-plus" aria-hidden="true"></i>Tambah Data</a> -->
        </div>
        <div class="panel-content pagination2">
          <table class="table table-bordered table-striped table-hover" id="data">
            <thead>
              <tr>
                <th class="header">No.</th>
                <th class="header">No Regis.</th>
                <th class="header">Nama</th>
                <th class="header">NIK</th>
                <th class="header">Email</th>
                <th class="header">No Telp</th>
                <th class="header">Status</th>
                <th class="header">Option</th>
              </tr>
            </thead>
            
            <tbody>
            <% for(var i=0; i<data.length; i++){ %>
              <tr>
                <td><%= i+1%></td>
                <td><%= data[i].no_regis%></td>
                <td><%= data[i].nama%></td>
                <td><%= data[i].nik%></td>
                <td><%= data[i].email%></td>
                <td><%= data[i].telp%></td>
                <td><%= (data[i].disetujui)?'Disetujui':'Ditolak'%></td>
                <td>
                <ul class="list-inline text-center">
                  <li><a class="btn btn-warning btn-embossed btn-sm" href="/peta_informasi_tata_ruang/edit/<%= data[i].id%>" role="button" title="Detail" ><i class="fa fa-info" aria-hidden="true"></i></a></li>
                  <li class="separator">|</li>
                  <li><a class="btn btn-danger btn-embossed btn-sm" href="/peta_informasi_tata_ruang/delete/<%= data[i].id%>" role="button" title="Hapus" ><i class="fa fa-trash-o" aria-hidden="true"></i></a></li>
                </ul>

                <ul class="list-inline text-center">
                  <li><a class="btn btn-info btn-embossed btn-sm"target="_blank" href="/peta_informasi_tata_ruang/cetak_rekom/<%= data[i].id%>" role="button"><i class="fa fa-print" aria-hidden="true"></i> Cetak Rekomendasi</a></li>
                  <li><a class="btn btn-info btn-embossed btn-sm"target="_blank" href="/peta_informasi_tata_ruang/cetak_lamp/<%= data[i].id%>" role="button"><i class="fa fa-print" aria-hidden="true"></i> Cetak Lampiran</a></li>

                </ul>
              </td>
              </tr>
              <% }%>
          
            </tbody>
          </table>
        </div>
      </div>
    </div>  
  </div>
  <div class="footer">
    <div class="copyright">
      <p class="pull-left sm-pull-reset">
        <span>Copyright <span class="copyright">Â©</span> 2017 </span>
        <span>Pemerintah Kabupaten Pekalongan</span>.
      </p>
    </div>
  </div>
</div>
<!-- END PAGE CONTENT -->

<% include ../../template-backoffice/footer.ejs %>

<script type="text/javascript">
  $(document).ready(function() {
    $('#data').DataTable({
      "language": {
          "sProcessing":   "Sedang memproses...",
          "sLengthMenu":   "_MENU_",
          "sZeroRecords":  "Tidak ditemukan data yang sesuai",
          "sInfo":         "Menampilkan _START_ sampai _END_ dari _TOTAL_ entri",
          "sInfoEmpty":    "Menampilkan 0 sampai 0 dari 0 entri",
          "sInfoFiltered": "(disaring dari _MAX_ entri keseluruhan)",
          "sInfoPostFix":  "",
          "sUrl":          "",
          "oPaginate": {
            "sFirst":    "Pertama",
            "sPrevious": "Sebelumnya",
            "sNext":     "Selanjutnya",
            "sLast":     "Terakhir"
          }
      },
      "columnDefs": [
          { "width": "110px", "targets": 4}
      ],
    });
  });
</script>